    },
    "scripts": {
        "start": "react-scripts start",
        "start:stg": "env-cmd -f .env.stg react-scripts start",
        "start:prd": "env-cmd -f .env.prd react-scripts start",
        "build": "react-scripts build",
        "build:stg": "env-cmd -f .env.stg react-scripts build",
        "build:prd": "env-cmd -f .env.prd react-scripts build",
        "test": "react-scripts test",
        "test:cov": "npm test -- --coverage --watchAll=false",
        "test:debug": "react-scripts --inspect-brk test --runInBand",
        "eject": "react-scripts eject",
        "changelog": "auto-changelog --starting-version 3.0.0 --commit-limit false --hide-credit",
