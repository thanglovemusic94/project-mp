    },
    "bugs": {
        "url": "https://github.com/coreui/coreui-free-react-admin-template/issues"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 10",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!**/*index.js",
            "!src/serviceWorker.js",
            "!src/polyfill.js"
        ]
    },
    "engines": {
        "node": ">=10",
        "npm": ">=6"
    }
